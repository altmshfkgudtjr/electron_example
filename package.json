{
  "name": "electron-example",
  "version": "1.0.0",
  "private": false,
  "main": "public/Main.js",
  "homepage": "./",
  "dependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.7",
    "immer": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.6",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "react-start": "BROWSER=none yarn start",
    "electron-start": "ELECTRON_START_URL=http://0.0.0.0:3000 electron .",
    "prebuild": "react-scripts test --watchAll=false",
    "build": "cross-env GENERATE_SOURCEMAP=false env-cmd -f .env && yarn build",
    "postbuild": "electron-builder build -c.extraMetadata.main=build/Main.js",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "cross-env": "^7.0.3",
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "env-cmd": "^10.1.0",
    "react-scripts": "4.0.1"
  },
  "proxy": "http://localhost:5000"
}
